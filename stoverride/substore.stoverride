#!desc=高级订阅管理工具 @Peng-YM
name: Sub-Store
desc: 高级订阅管理工具 @Peng-YM
http:
  mitm:
    - 'sub.store'
  script:
    - match: ^https?:\/\/sub\.store
      name: Sub-Store
      type: request
      require-body: true
      timeout: 10
script-providers:
  Sub-Store:
    url: https://raw.githubusercontent.com/Peng-YM/Sub-Store/master/backend/sub-store.min.js,requires-body=true,timeout=120,max-size=131072
    interval: 86400
